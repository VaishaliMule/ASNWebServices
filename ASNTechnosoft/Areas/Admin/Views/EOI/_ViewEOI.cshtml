                        <table class="table table-bordered">
                            <tr>
                                <th>Institute Name</th>
                                <td> @Html.DisplayFor(model => model.InstituteName)</td>
                            </tr>
                            <tr>
                                <th>Subcribed Service Name</th>
                                <td>@Html.DisplayFor(model => model.ServiceName)</td>
                            </tr>
                            <tr>
                                <th>Subcribed Date</th>
                                <td>@Html.DisplayFor(model => model.SubcribedDate)</td>
                            </tr>
                            <tr>
                                <th>Quantity</th>
                                <td>@Html.DisplayFor(model => model.Quantity)</td>
                            </tr>
                            <tr>
                                <th>Amount</th>
                                <td>@Html.DisplayFor(model => model.Amount)</td>
                            </tr>
                            <tr>
                                <th>Renewal Date</th>
                                <td>@Html.DisplayFor(model => model.RenewalDate)</td>
                            </tr>
                            
                        </table>


                    </div>
