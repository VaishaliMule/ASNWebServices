@model IEnumerable<TechnosoftModel.InstituteDetails_All_Result>

@{
    ViewBag.Title = "InActiveInstitute";
    Layout = "~/Areas/ASNAdmin/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html lang="en">
<!--begin::Head-->
<head>


    <link href="/Content/dist/assets/plugins/custom/datatables/datatables.bundle.css?v=7.0.4" rel="stylesheet" type="text/css" />

    <style>
        tr:nth-child(even) {
            background-color: #f2f2f2;
        }
    </style>
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" class="header-fixed header-mobile-fixed subheader-enabled page-loading" style="overflow:scroll">
    <div class="subheader py-1 py-lg-2 subheader-transparent" id="kt_subheader">
        <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline mr-5">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold my-2 mr-5">Dashboard</h5>
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">In-Active Institute</a>
                        </li>

                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->

        </div>
    </div>

    <div class="container">
        <!--begin::Card-->
        <div class="card">
            <div class="card-header flex-wrap border-0 pt-6 pb-0">
                <div class="card-title">
                    <h3 class="card-label">
                        Trial based Institute
                    </h3>
                </div>
                
                <div id="buttons"></div>
            </div>

            
            <div class="card-body">
                <!--begin: Datatable table table-bordered table-checkable dataTable no-footer -->
                <table class="table table-bordered table-hover table-checkable dataTable no-footer collapsed" id="coursedt">
                    <thead>
                        <tr style="background-color:cadetblue">
                            <th>
                                <label class="font-weight-boldest text-dark">SN</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Id</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Registration Date</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Last Login Date</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Institute Name</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Owner Name</label>
                            </th>

                            <th>
                                <label class="font-weight-boldest text-dark">Contact number</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Email</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">District</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Registered Student</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Admission</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Wallet Balance</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Payment</label>
                            </th>
                            <th>
                                <label class="font-weight-boldest text-dark">Status</label>
                            </th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.INSTITUTEID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.REGISTRATIONDATE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LASTLOGINDATE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.INSTITUTENAME)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.OWNERNAME)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MOBILE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.EMAIL)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DISTRICT)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TOTALSTUDENT)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TOTALADMISSION)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.WALLETBALANCE)
                                </td>
                                <td>
                                    @if (item.EOISTATUS == "1")
                                    {
                                        <P class="text-success">Completed</P>
                                    }
                                    else
                                    {
                                        <P class="text-danger">Pending</P>
                                    }
                                </td>

                                <td>
                                    @if (item.IMSSTATUS == "Trial")
                                    {
                                        <P class="text-info">@item.IMSSTATUS</P>
                                    }
                                    @if (item.IMSSTATUS == "Disable")
                                    {
                                        <P class="text-danger">@item.IMSSTATUS</P>
                                    }
                                    @if (item.IMSSTATUS == "Enable")
                                    {
                                        <P class="text-success">@item.IMSSTATUS</P>
                                    }
                                </td>
                                <td>
                                    <a href="@Url.Action("AddFollowup","Dashboard",new { id=@item.INSTITUTEID})" data-placement="left" title="Add Followup" data-toggle="tooltip" data-theme="dark" class="btn btn-sm btn-clean btn-icon mr-2 btn-primary">
                                        <i class="flaticon2-calendar-6"></i>
                                    </a>
                                    <a href="@Url.Action("ViewFollowup","Dashboard",new { id=@item.INSTITUTEID})" data-placement="left" title="View Followup" data-toggle="tooltip" data-theme="dark" class="btn btn-sm btn-clean btn-icon mr-2 btn-success">
                                        <i class="flaticon-eye"></i>
                                    </a>

                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                <!--end: Datatable-->
                <div class="d-flex justify-content-between border-top mt-5 pt-10">
                    <div>
                        <a href="@Url.Action("Index","Dashboard")" id="backLink" class="btn btn-bg-danger font-weight-bold text-uppercase px-9 py-4">Back to Dashboard</a>
                    </div>
                </div>
            </div>
        </div>
        <!--end::Card-->
    </div>


    <script src="~/Content/dist/assets/plugins/global/plugins.bundle.js?v=7.0.4"></script>
    <script src="~/Content/dist/assets/plugins/custom/prismjs/prismjs.bundle.js?v=7.0.4"></script>
    <script src="~/Content/dist/assets/js/scripts.bundle.js?v=7.0.4"></script>

    <script src="~/Content/dist/assets/plugins/custom/datatables/datatables.bundle.js?v=7.0.4"></script>
    <script src="~/Content/dist/assets/js/pages/crud/datatables/basic/scrollable.js?v=7.0.4"></script>
    <script>
        jQuery(document).ready(function ($) {
            var table = $('#coursedt').DataTable({
                "pageLength": 25,
                scrollY: '500vh',
                scrollX: true,
                scrollCollapse: true,
            });
            var buttons = new $.fn.dataTable.Buttons(table, {
                buttons: [
                    {
                        extend: 'collection',
                        text: '<span class="svg-icon svg-icon-md svg-icon-primary"><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><rect x="0" y="0" width="24" height="24" /><path d="M3,16 L5,16 C5.55228475,16 6,15.5522847 6,15 C6,14.4477153 5.55228475,14 5,14 L3,14 L3,12 L5,12 C5.55228475,12 6,11.5522847 6,11 C6,10.4477153 5.55228475,10 5,10 L3,10 L3,8 L5,8 C5.55228475,8 6,7.55228475 6,7 C6,6.44771525 5.55228475,6 5,6 L3,6 L3,4 C3,3.44771525 3.44771525,3 4,3 L10,3 C10.5522847,3 11,3.44771525 11,4 L11,19 C11,19.5522847 10.5522847,20 10,20 L4,20 C3.44771525,20 3,19.5522847 3,19 L3,16 Z" fill="#000000" opacity="0.3" /><path d="M16,3 L19,3 C20.1045695,3 21,3.8954305 21,5 L21,15.2485298 C21,15.7329761 20.8241635,16.200956 20.5051534,16.565539 L17.8762883,19.5699562 C17.6944473,19.7777745 17.378566,19.7988332 17.1707477,19.6169922 C17.1540423,19.602375 17.1383289,19.5866616 17.1237117,19.5699562 L14.4948466,16.565539 C14.1758365,16.200956 14,15.7329761 14,15.2485298 L14,5 C14,3.8954305 14.8954305,3 16,3 Z" fill="#000000" /></g></svg></span>Export',
                        className: 'btn-primary',
                        buttons: [
                            {
                                extend: 'print',
                                text: '<i class="la la-print text-success"></i> PRINT',
                                titleAttr: 'PRINT'
                            },
                            {
                                extend: 'pdfHtml5',
                                text: '<i class="la la-file-pdf-o text-danger"></i> PDF',
                                titleAttr: 'PDF'
                            },
                            {
                                extend: 'excelHtml5',
                                text: '<i class="la la-file-excel-o text-success"></i> EXCEL',
                                titleAttr: 'Excel'
                            },
                            {
                                extend: 'csvHtml5',
                                text: '<i class="la la-file-text-o text-primary"></i> CSV',
                                titleAttr: 'CSV'
                            },
                            {
                                extend: 'copyHtml5',
                                text: '<i class="la la-copy text-warning"></i> COPY',
                                titleAttr: 'Copy',
                                className: 'blue'
                            },
                        ]
                    }

                ]
            }).container().appendTo($('#buttons'));

        });
    </script>

    <script type="text/javascript">
        history.pushState(null, null, location.href);
        window.onpopstate = function () {
            history.go(1);
        };
    </script>
    @if (TempData["Message"] != null)
    {
        <script type="text/javascript">
                $(document).ready(function () {
                    @Html.Raw(TempData["Message"]);
                });
        </script>
        TempData["Message"] = null;
    }
</body>
<!--end::Body-->
</html>


